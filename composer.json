{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "aymanalhattami/filament-page-with-sidebar": "^2.6",
        "bezhansalleh/filament-language-switch": "^3.1",
        "bezhansalleh/filament-panel-switch": "^1.1",
        "bezhansalleh/filament-shield": "^3.3",
        "coolsam/flatpickr": "^3.1",
        "deepseek-php/deepseek-laravel": "^2.0",
        "devonab/filament-easy-footer": "^1.0",
        "filament/filament": "^3.2.0",
        "filament/infolists": "^3.2",
        "filament/notifications": "^3.2",
        "filament/spatie-laravel-settings-plugin": "^3.2",
        "firebase/php-jwt": "^6.11",
        "hasnayeen/themes": "^3.0",
        "hayderhatem/filament-sub-navigation": "^1.0",
        "joaopaulolndev/filament-edit-profile": "^1.0",
        "kenepa/banner": "^0.0.9",
        "laravel/framework": "^11.9",
        "laravel/tinker": "^2.9",
        "maatwebsite/excel": "^3.1",
        "malzariey/filament-daterangepicker-filter": "^3.1",
        "mpdf/mpdf": "^8.2",
        "oriondevelops/filament-greeter": "^0.2.0",
        "parfaitementweb/filament-country-field": "^2.2",
        "petercoles/multilingual-country-list": "^1.2",
        "phpoffice/phpspreadsheet": "^1.30",
        "psr/simple-cache": "2.0",
        "pxlrbt/filament-excel": "^2.3",
        "spatie/laravel-medialibrary": "^11.10",
        "swisnl/filament-backgrounds": "^1.1",
        "tecnickcom/tcpdf": "^6.10",
        "teguh02/indonesia-territory-forms": "^1.1",
        "torgodly/html2media": "^1.1",
        "ysfkaya/filament-phone-input": "^3.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "jn-jairo/laravel-ngrok": "^3.0",
        "laravel/pail": "^1.1",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^11.0.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
